/ Question No.1
// Take 5 Subject Marks each subject 100 marks; Calculate the percentage; based on the percentage print the grade and the percentage


package com.Percentage;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	 int marks[] = new int[5];
	 int i;
	 float per, sum=0;

	 Scanner sc = new Scanner(System.in);

	 System.out.print("Enter The 5 Subject Marks :");
	 for (i=0; i<5; i++);
        {
            marks[i]= sc.nextInt();
            sum = sum + marks[i];
        }

        per = (sum/500 * 100);

        System.out.print("\nStudent Percentage is");





    }
}

