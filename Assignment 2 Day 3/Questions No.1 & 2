// Question No.2


String employeename="sravan kumar";
int date=11;  
int month=9;
int  year=1994;
float monthlysalary=30000;
float annualsalary=12*monthlysalary;
int age=2020-year;
float annual;

if (annualsalary > 200000) {

annual= (annualsalary *5)/(100+ annualsalary);
}
else if (annualsalary > 300000) {
 annual= (annualsalary *10)/(100+ annualsalary);
} 
else if(annualsalary > 400000) {
 annual= (annualsalary *15)/(100+ annualsalary);
}
else  {
 annual= (annualsalary *20)/(100+ annualsalary);
}

System.out.println("name:"+ employeename+" age:"+age+" salar:"+annualsalary+" tax:"+annual);
